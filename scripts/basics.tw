# basics.tw
var1 := 1;
var2 := 2;

someFunc := action() {
	print("var1: ", var1);
	print("var2: ", var2);
}

someObject := object() {
	var1 := 3 + 25 - (2 * 5);
	var2 := 4;

	someMethod := action() {
		print("var1: ", var1);
		print("var2: ", var2);
	}
}

someObject.var1 = 5;
someObject.var2 = 6;

if(someObject["var1"] == someObject.var1) {
	print("someObject[\"var1\"] is the same as someObject.var1");
} 

someObject.someMethod();
someObject["someMethod"]();
someFunc();